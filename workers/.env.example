ENV=development
REDIS_URL=redis://redis:6379/0
DATABASE_URL=postgresql+psycopg://postgres:postgres@postgres:5432/devlens
QDRANT_URL=http://qdrant:6333
QDRANT_COLLECTION=devlens_code_chunks
PARSE_CLONE_TIMEOUT_SECONDS=60
